{
  "num_prompts": 5,
  "prompts": [
    "prompt_voyager_1.md",
    "prompt_voyager_2.md", 
    "prompt_voyager_3.md",
    "prompt_voyager_4.md",
    "prompt_voyager_5.md"
  ],
  "assistants": [
    "Claude Code",
    "Augment CLI"
  ],
  "runs_per_prompt": 2,
  "output_filename": "voyager_benchmark_results.json",
  "metrics": [
    "response_time",
    "output_quality",
    "output_format_success", 
    "instruction_adherence",
    "context_adherence"
  ],
  "metrics_config": {
    "agent_success": { "threshold": 7, "mode": "quality" },
    "output_format": { "regex": "^.{1,}$" }
  }
}
