{
  "name": "augbench",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "description": "AI Coding Assistant Benchmarking CLI",
  "bin": {
    "augbench": "bin/augbench.js"
  },
  "scripts": {
    "start": "node bin/augbench.js",
    "help": "node bin/augbench.js help",
    "validate": "node bin/augbench.js validate",
    "benchmark": "node bin/augbench.js benchmark",
    "test": "node --test src/tests/**/*.test.js",
    "pr-metrics": "python3 roi_scripts/github_pr_metrics.py"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "axios": "^1.0.0",
    "chart.js": "^4.0.0",
    "chartjs-node-canvas": "^5.0.0",
    "dotenv": "^16.4.5",
    "fs-extra": "^11.0.0",
    "joi": "^17.0.0",
    "simple-git": "^3.0.0",
    "web-tree-sitter": "^0.24.7"
  }
}
